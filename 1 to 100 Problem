/*Write a program that use console.log to print all the numbers from 1 to
100 with two exceptions. For numbers divisible by 3, print "Fizz" instead
of the number, and for numbers divisible by 5(and not by 3) print Buzz.*/

for(var num = 0, num => 100, num++){

  if(var num % 3 == 0){console.log("Fizz")}
  else if(var num % 5 == 0 && var num % 3 == 1 || 2)
  {console.log(Buzz)}

  }

  else {console.log(var num);

  }

}
